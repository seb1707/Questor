  <addWebifiersToDronePriorityTargetList>true</addWebifiersToDronePriorityTargetList>

  <!-- This logs to the console (and console log) the current PrimaryWeapon and Drone priorityTargets every x seconds, whatever you specify -->
  <listPriorityTargetsEveryXSeconds>300</listPriorityTargetsEveryXSeconds>

  <!-- MaxRange we should use NOS / Neuts -->
  <NosDistance>38000</NosDistance>
  
  <!-- Maximum number of targets per value, these will work better set to a higher number but you need multitasking trained and enough targeting slots to accomidate these high and low value slots plus the maximumWreckTarget slots  all at once -->
  <maximumHighValueTargets>3</maximumHighValueTargets><!-- default is 3 -->
  <maximumLowValueTargets>2</maximumLowValueTargets><!-- default is 2 -->

