  <debugDoneState>false</debugDoneState>
  <debugDrones>false</debugDrones>
  <debugDroneHealth>false</debugDroneHealth>
  <debugExceptions>false</debugExceptions>
  <debugFittingMgr>false</debugFittingMgr>
  <debugGetBestTarget>false</debugGetBestTarget>
  <debugGetBestDroneTarget>false</debugGetBestDroneTarget>
  <debugFleetSupportMaster>false</debugFleetSupportMaster>
  <debugFleetSupportSlave>false</debugFleetSupportSlave>
  <debugGotobase>false</debugGotobase>
